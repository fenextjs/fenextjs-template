{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "preserve",
    "esModuleInterop": true,
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "fenextjs/*": [
        "../../fenextjs/fenextjs/*"
      ],
      "fenextjs-component/*": [
        "../../fenextjs/fenextjs-component/*"
      ],
      "fenextjs-date/*": [
        "../../fenextjs/fenextjs-date/*"
      ],
      "fenextjs-documentation/*": [
        "../../fenextjs/fenextjs-documentation/*"
      ],
      "fenextjs-error/*": [
        "../../fenextjs/fenextjs-error/*"
      ],
      "fenextjs-export/*": [
        "../../fenextjs/fenextjs-export/*"
      ],
      "fenextjs-firebase/*": [
        "../../fenextjs/fenextjs-firebase/*"
      ],
      "fenextjs-functions/*": [
        "../../fenextjs/fenextjs-functions/*"
      ],
      "fenextjs-hook/*": [
        "../../fenextjs/fenextjs-hook/*"
      ],
      "fenextjs-img-placeholder/*": [
        "../../fenextjs/fenextjs-img-placeholder/*"
      ],
      "fenextjs-interface/*": [
        "../../fenextjs/fenextjs-interface/*"
      ],
      "fenextjs-svg/*": [
        "../../fenextjs/fenextjs-svg/*"
      ],
      "fenextjs-validator/*": [
        "../../fenextjs/fenextjs-validator/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
